<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/rl_mian" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.quvideo.xiaoying.editorx.board.effect.ui.SimpleIconTextView android:layout_gravity="center" android:id="@+id/sitv_reset" android:visibility="gone" android:layout_width="64dp" android:layout_height="match_parent" android:layout_centerVertical="true" android:layout_marginEnd="15dp" android:layout_alignParentEnd="true" app:sitv_bottom_text="@string/xiaoying_str_edit_reset_text" app:sitv_top_drawable="@drawable/editorx_ico_effect_reset"/>
    <com.quvideo.xiaoying.editorx.widget.trimBar.TrimBarView android:id="@+id/trim_bar_view" android:visibility="gone" android:layout_width="match_parent" android:layout_height="36dp" android:layout_marginBottom="60dp" android:layout_alignParentBottom="true"/>
    <androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/board_mosaic" android:background="@drawable/editorx_drawer_bg" android:clickable="true" android:layout_width="match_parent" android:layout_height="@dimen/editor_board_whole_height" android:layout_alignParentBottom="true">
        <com.quvideo.xiaoying.xyui.view.CustomHandleView android:id="@+id/handle_view" android:layout_width="match_parent" android:layout_height="wrap_content" app:chv_title_text="@string/xiaoying_str_ve_mosaic_title" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toTopOf="0"/>
        <RelativeLayout android:gravity="center_horizontal" android:id="@+id/rl_content" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="80dp" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintTop_toTopOf="0">
            <FrameLayout android:id="@+id/group_blur" android:background="@drawable/editorx_mosaic_item_bg" android:layout_width="88dp" android:layout_height="88dp" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintTop_toTopOf="0">
